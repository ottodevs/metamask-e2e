{
  "name": "metamask-puppeteer",
  "version": "0.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "build":"yarn prepare-metamask && tsc",
    "prepare-metamask": "rm -rf ./metamask-bundle && ./scripts/get-metamask.sh && ./scripts/fix-metamask-manifest.js",
    "tslint": "tslint -p ./tsconfig.json -e 'node_modules/**/*' -e '**/node_modules/**/*' '**/*.ts'",
    "tslint:fix": "tslint --fix --format stylish -p ./tsconfig.json -e 'node_modules/**/*' -e '**/node_modules/**/*' '**/*.ts'",
    "tsc": "tsc --noEmit",
    "lint": "yarn tslint && yarn tsc",
    "lint:fix": "yarn tslint:fix && yarn tsc",
    "test": "yarn lint",
    "test:fix": "yarn lint:fix"
  },
  "peerDependencies": {
    "puppeteer": "^1.10.0"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.118",
    "@types/node": "^10.12.0",
    "@types/puppeteer": "^1.10.0",
    "ts-essentials": "^1.0.0",
    "ts-node": "^7.0.1"
  }
}
